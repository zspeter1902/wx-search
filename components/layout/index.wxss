.container {
  position: relative;
  min-height: 100vh;
  background-color: #fff;
  overflow: hidden;
}
.bg-top {
  position: absolute;
  top: 0;
  right: 0;
  width: 365rpx;
  height: 358rpx;
  z-index: 1;
}
.content {
  position: relative;
  padding: 76rpx 36rpx 0;
  z-index: 9;
}
.bg-bottom {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 400rpx;
  z-index: 1;
}